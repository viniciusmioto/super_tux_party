window/size/viewport_height=720
window/vsync/vsync_mode=false

[editor_plugins]

enabled=PackedStringArray("res://addons/minigame_config_dialog/plugin.cfg")

[gui]

theme/custom="res://assets/defaults/default_theme.tres"
theme/use_hidpi=true

