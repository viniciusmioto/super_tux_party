msgid "MINIGAME_ACTION_LEFT"
msgstr "Mover para a esquerda"

msgid "MINIGAME_ACTION_RIGHT"
msgstr "Mover para a direita"

msgid "MINIGAME_ACTION_JUMP"
msgstr "Saltar"

msgid "MINIGAME_ACTION_THROW"
msgstr "Lance bola"

msgid "BOWLING_FINISH_MSG"
msgstr "Acabou!"

