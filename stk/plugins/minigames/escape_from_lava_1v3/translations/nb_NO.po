msgid "ESCAPE_FROM_LAVA_FINISH_MSG"
msgstr "Slutt!"

msgid "MINIGAME_DESCRIPTION"
msgstr ""
"Spillere kappes mot hverandre i et løp til toppen, mens lava stiger under "
"dem.\n"
"De må forsere flere dører. Én av disse dørene er blokkert og vil ikke åpne "
"seg.\n"
"Den første spilleren som kommer seg trygt til toppen vinner."

msgid "MINIGAME_ACTION_OBSTRUCT_THREE"
msgstr "Blokker dør 3 - kun enkeltspiller"

msgid "MINIGAME_ACTION_OBSTRUCT_TWO"
msgstr "Blokker dør 2 - kun enkeltspiller"

msgid "MINIGAME_ACTION_OBSTRUCT_ONE"
msgstr "Blokker dør 1 - kun enkeltspiller"

msgid "MINIGAME_NAME"
msgstr "Flykt fra lava"

msgid "MINIGAME_ACTION_RIGHT"
msgstr "Flytt til høyre"

msgid "MINIGAME_ACTION_LEFT"
msgstr "Flytt til venstre"

msgid "MINIGAME_ACTION_DOWN"
msgstr "Flytt nedover"

msgid "MINIGAME_ACTION_UP"
msgstr "Flytt oppover"

msgid "MINIGAME_ACTION_OBSTRUCT"
msgstr "Blokker døren"
